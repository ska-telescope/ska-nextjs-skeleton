{
  "name": "ska-nextjs-skeleton",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:debug": "next build --debug",
    "build:profile": "next build --profile",
    "export": "next build && next export",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest --watch",
    "code-analysis": "jest --coverage",
    "test:coverage": "jest --coverage",
    "test:report": "jest --coverage --report-dir=build/reports --reporter=html",
    "test:report:ci": "jest --coverage --report-dir=build/reports --reporter=cobertura",
    "test:summary": "jest --coverage --reporter=text-summary",
    "skao:update": "yarn add @ska-telescope/ska-gui-components@latest",
    "start": "NODE_ENV=production node ./server.js",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "cypress ci",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:ci": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\" --headless --browser chrome --reporter junit --config video=false --reporter-options mochaFile=build/tests/unit-tests[hash].xml",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e --headless\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component --headless"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.12",
    "@mui/material": "^5.14.12",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "next": "^13.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.8",
    "typescript": "^5.2.2",
    "zod-formik-adapter": "^1.2.0"
  },
  "devDependencies": {
    "@cypress/react18": "^2.0.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "cypress": "^13.3.0",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "start-server-and-test": "^2.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}
